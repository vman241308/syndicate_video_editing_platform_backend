{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"lib": [
			"ES2020",
			"DOM"
		],
		"allowJs": true,
		"outDir": "build",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"plugins": [
			{
				"transform": "typia/lib/transform"
			}
		]
	},
	"extends": "@tsconfig/node16/tsconfig.json"
}
